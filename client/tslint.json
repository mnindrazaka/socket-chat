{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react"],
  "jsRules": {},
  "rules": {
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "semicolon": {
      "options": "never"
    },
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "interface-name": true,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
